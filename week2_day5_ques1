import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {
    public static void main(String[] args) {
    Scanner obj = new Scanner(System.in);
    float color1=obj.nextFloat();
    float color2=obj.nextFloat();
    float color3=obj.nextFloat();

    int color1Int=(int)Math.ceil(color1);
    int color2Int=(int)Math.ceil(color2);
    int color3Int=(int)Math.ceil(color3);
    System.out.println(color1Int + " " + color2Int + " " + color3Int);
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
    }
}
